<?xml version="1.0"?>
    <page layout="3columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
        <body>
            <referenceBlock name="register-link" remove="true" />
            <referenceBlock name="my-account-link" remove="true" /> 
           <referenceBlock name="wish-list-link" remove="true" />
            <move element="minicart" destination="header.panel" before="-"/> 
             <move element="header" destination="header.panel" before="authorization-link-login"/>
            <move element="catalog.topnav" destination="sidebar.main" />
            <referenceContainer name="header.panel">
                <block class="Magento\Theme\Block\Html\Header" name="header" template="html/header.phtml"/>
            </referenceContainer>
            <referenceContainer name="header-wrapper">
                <block class="Magento\Theme\Block\Html\Header" name="link" template="html/link.phtml"/>
            </referenceContainer>
             <referenceContainer name="footer-container">
                <block class="Magento\Theme\Block\Html\Footer" name="footer" template="html/footer.phtml"/>
            </referenceContainer>
            <referenceContainer name="header-wrapper"> 
                <block class="Magento\Cms\Block\Block" name="header-link-1"> 
                    <arguments> 
                        <argument name="Cake header" xsi:type="string">header-link-1</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="header-link-2"> 
                    <arguments> 
                        <argument name="Cake header" xsi:type="string">header-link-2</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="header-link-3"> 
                    <arguments> 
                        <argument name="Cake header" xsi:type="string">header-link-3</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="header-link-4"> 
                    <arguments> 
                        <argument name="Cake header" xsi:type="string">header-link-4</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="header-link-5"> 
                    <arguments> 
                        <argument name="Cake header" xsi:type="string">header-link-5</argument> 
                    </arguments> 
                </block>
            </referenceContainer>
            
            <referenceContainer name="footer-container"> 
                <block class="Magento\Cms\Block\Block" name="delilahs_social_icon"> 
                    <arguments> 
                        <argument name="Delilahs Social" xsi:type="string">delilahs_social_icon</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_social_link"> 
                    <arguments> 
                        <argument name="Delilahs Social" xsi:type="string">delilahs_social_link</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_newsletter"> 
                    <arguments> 
                        <argument name="Delilahs newsletter" xsi:type="string">delilahs_newsletter</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_logo"> 
                    <arguments> 
                        <argument name="Delilahs logo" xsi:type="string">delilahs_logo</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delliahs_para"> 
                    <arguments> 
                        <argument name="Delliahs para" xsi:type="string">delliahs_para</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_sub"> 
                    <arguments> 
                        <argument name="Delilahs sub" xsi:type="string">delilahs_sub</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_bmmi"> 
                    <arguments> 
                        <argument name="Delilahs bmmi" xsi:type="string">delilahs_bmmi</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_visit"> 
                    <arguments> 
                        <argument name="Delilahs visit" xsi:type="string">delilahs_visit</argument> 
                    </arguments> 
                </block>
                <block class="Magento\Cms\Block\Block" name="delilahs_copyright"> 
                    <arguments> 
                        <argument name="Delilahs Copyright" xsi:type="string">delilahs_copyright</argument> 
                    </arguments> 
                </block>
            </referenceContainer> 
        </body>
    </page> 
